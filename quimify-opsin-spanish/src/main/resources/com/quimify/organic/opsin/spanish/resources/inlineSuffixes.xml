<!DOCTYPE tokenLists SYSTEM "tokenLists.dtd">
<!-- value = a reference to suffixApplicability.xml -->
<tokenLists>
	<tokenList tagname="suffix" type="inline" symbol="i">
		<token value="oyl" additionalValue="aldehyde">aldehidoil</token>
		<token value="amido">amido|amidil</token>
		<token value="aminyl">aminil</token>
		<token value="oyl" additionalValue="amide">amoil</token>
		<token value="oyl" additionalValue="anilide">aniloil</token>
		<token value="carbonimidoyl">carbonimidoil</token>
		<token value="carbonyl">carbonil</token>
		<token value="hydrazido">hidracido</token>
		<token value="io">io</token>
		<token value="iminyl">iminil</token>
		<token value="oximino">oximino</token>
		<token value="oxy">oxi</token>
		<token value="oyl">oil</token>
		<token value="selenenyl">selenenil</token>
		<token value="seleninyl">seleninil</token>
		<token value="selenonyl">selenonil</token>
		<token value="sulfenamido">sulfenamido</token>
		<token value="sulfenoselenoyl">sulfenoselenoil</token>
		<token value="sulfenothioyl">sulfenotioil</token>
		<token value="sulfenyl">sulfenil</token>
		<token value="sulfinyl">sulfinil</token>
		<token value="sulfonyl">sulfonil</token>
		<token value="tellurenyl">telurenil</token>
		<token value="tellurinyl">telurinil</token>
		<token value="telluronyl">teluronil</token>
		<token value="yl">ilo|il</token>
		<token value="ylidene">ilideno|iliden</token>
		<token value="ylidyne">ilidino|ilidin</token>
	</tokenList>

	<!--Have two attachement points as indicated by the subType-->
	<tokenList tagname="suffix" type="inline" subType="cycleformer" symbol="i">
		<token value="imido">imido|imidil</token>
	</tokenList>

	<tokenList tagname="suffix" type="inline" symbol="w"><!--are allowed after another inline suffix, only the basic inline suffixes-->
		<token value="oyl">oil</token>
		<token value="yl">ilo|il</token>
		<token value="ylidene">ilideno|iliden</token>
		<token value="ylidyne">ilidino|ilidin</token>
	</tokenList>

	<tokenList tagname="suffix" type="inline" symbol="¶"><!--inline suffixes allowing infixes -->
		<token value="amido">amido|amidil</token>
		<token value="carbonyl">carbonil</token>
		<token value="hydrazido">hidracido</token>
		<token value="oxy">oxi</token>
		<token value="oyl">oil</token>
	</tokenList>

	<tokenList tagname="suffix" type="inline" symbol="Û"><!--inline suffixes allowing infixes and suffix prefixes-->
		<token value="amido">amido|amidil</token>
		<token value="hydrazido">hydracido</token>
		<token value="oyl">oil</token>
	</tokenList>

	<tokenList tagname="suffix" type="inline" symbol="»"><!--used for amino acids and carbohydrates-->
		<token value="locantedAminoAcidOrCarbohydrateYl">ilo|il</token>
		<token value="locantedAminoAcidOrCarbohydrateYlidene">ilideno|iliden</token>
	</tokenList>

	<tokenList tagname="suffix" type="inline" symbol="×"><!--ylene-->
		<!--special cased to be the same as diyl-->
		<token value="yl" additionalValue="yl">ileno|ilen</token>
	</tokenList>
</tokenLists>
